def longestCommonPrefix(strs):
    strs = sorted(strs)
    
    first = strs[0]
    last = strs[-1]
    
    res = ""
    for i in range(len(first)):
        if first[i] == last[i]:
            res += first[i]
        else:
            break
    return res
        
            
        


