def pascals_triangle(N):
    arr = []
    for i in range(1,N+1):
        arr.append([1]*i)
        if i>2:
            for j in range(1,i-1):
                arr[-1][j] = arr[-2][j] + arr[-2][j-1]
                
    return arr



