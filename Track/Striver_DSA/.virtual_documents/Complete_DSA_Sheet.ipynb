def maxOccured(L,R,N,maxx):
        arr = [0 for _ in range(maxx+2)]
        
        for i in range(N):
            arr[L[i]] += 1
            arr[R[i]+1] -= 1
        
        # calculate prefix sums
        csum = arr[0]
        for i in range(1,len(arr)):
            arr[i] += arr[i-1]

        res, max_count = 0, arr[0]
        for i in range(1,len(arr)):
            if arr[i] > max_count:
                res = i
                max_count = arr[i]

        return res


