def majorityElement(arr):
  n = len(arr)
  count = 0
  maj_ele = 0
  
  for x in arr:
      if count == 0: # some other ele occurs >= current maj_ele
          maj_ele = x
          count = 1
      elif x == maj_ele: # same ele as maj_ele
          count += 1
      else: # some different element
          count -= 1
  
  maj_count = 0
  for x in arr:
      if x == maj_ele:
          maj_count += 1

  return maj_ele if maj_count > (n//2) else -1
  


