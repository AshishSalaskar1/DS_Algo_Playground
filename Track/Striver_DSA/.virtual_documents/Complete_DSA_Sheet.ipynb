            return 
        
        self.inorder(node.left, k)
        self.count += 1
        if self.count == k:
            self.ans = node.val
            return
        self.inorder(node.right, k)
        
    def kthSmallest(self, root, k):
        self.count = 0
        self.inorder(root, k)
        return self.ans
        


