def minFallingPathSum( matrix) -> int:
    n = len(matrix)
    dp = [[0 for _ in range(n)] for _ in range(n)]
    dp[0] = matrix[0]
    
    for i in range(1,n):
        for j in range(n):
            # up direction is gauranteed, but diagonal up-left/right may overflow
            diag1 = float('inf')
            diag2 = float('inf')
            
            if (j-1) >= 0 and (j-1) < n:
                diag1 = dp[i-1][j-1]
            if (j+1) >= 0 and (j+1) < n:
                diag2 = dp[i-1][j+1]
            
            dp[i][j] = matrix[i][j] + min([dp[i-1][j], diag1, diag2])
            
    return min(dp[-1])
    


