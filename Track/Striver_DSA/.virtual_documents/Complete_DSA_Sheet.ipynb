    firstOcc = -1

    while lo<=hi:
        mid = lo + ((hi-lo)//2)
        if arr[mid]==target: # if same store and move left-side
            firstOcc = mid
            hi = mid-1
        elif arr[mid] > target:
            hi = mid-1
        else:
            lo = mid+1

    return firstOcc


