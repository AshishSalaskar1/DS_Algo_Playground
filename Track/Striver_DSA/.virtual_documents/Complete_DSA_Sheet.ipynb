def sort_01(arr):
    n = len(arr)
    zero_ptr = 0
    one_ptr = n-1
    cur = 0

    while cur < one_ptr:
        if arr[cur] == 1:
            arr[cur], arr[one_ptr] = arr[one_ptr], arr[cur] 
            one_ptr -= 1
        else:
            cur += 1
    return arr


