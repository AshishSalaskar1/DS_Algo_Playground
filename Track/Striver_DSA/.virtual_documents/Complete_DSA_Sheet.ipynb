def get_pivot_index(arr):
    n = len(arr)
    lo = 0
    hi = n-1

    while lo<hi:
        mid = lo + ((hi-lo)//2)
        if arr[mid] >= arr[0]:
            lo = mid+1
        else:
            hi = mid #cur ele can be res, or some elements may be in left side

    return lo

