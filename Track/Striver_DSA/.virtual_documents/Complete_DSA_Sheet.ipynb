def two_sum_sorted(arr, target):
    n = len(arr)
    lo = 0 
    hi = n-1

    while lo<hi:
        cSum = arr[lo]+arr[hi]

        if cSum == target:
            return lo,hi
        elif cSum > target:
            hi -= 1
        else:
            lo += 1
    return -1,-1


