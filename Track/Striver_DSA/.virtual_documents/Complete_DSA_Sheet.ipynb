def max_even_odd_subarray(arr):
    n = len(arr)
    res = 1
    cur_len = 1

    for i in range(n):
        # check if cur element can be added to older sequence
        if (arr[i]%2 == 0 and arr[i-1]%2 != 0) or (arr[i]%2 != 0 and arr[i-1]%2 == 0): # can be added
            cur_len += 1
            res = max(res, cur_len)
        else: #start new subarray from here
            cur_len = 1
    return res



