def bin_search(arr, target, start=None, end=None) -> int:
    n = len(arr)
    lo = start if start else 0
    hi = end if end else n-1

    if hi == lo and arr[hi]==target: # IMPORTANT -> [5], [4] 
        return lo

    while lo <= hi: # IMPORTANT -> (use <= if key lies first or last index)
        mid = lo + ((hi-lo)//2)
        if arr[mid] == target:
            return mid
        elif arr[mid] > target:
            hi = mid-1
        else:
            lo = mid+1
    return -1



