def longestCommonPrefix(strs):
        prev_word = strs[0]
        
        if len(strs) == 1:
            return strs[0]
        
        for word in strs[1:]:
            i,j = 0, 0
            prefix = ""
            while i<len(word) and j<len(prev_word) and word[i]==prev_word[j]:
                prefix += word[i]
                i += 1
                j += 1
            prev_word = prefix
                
        return prefix


